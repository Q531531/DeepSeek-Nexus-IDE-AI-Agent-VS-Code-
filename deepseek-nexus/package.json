{
  "name": "xiongdanqingcode",
  "displayName": "danqingCode",
  "description": "danqingCode - AI-powered coding assistant",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "deepseek-nexus.showChat",
        "title": "DeepSeek: Show Chat",
        "category": "DeepSeek Nexus"
      }
    ],
    "views": {
      "deepseek-nexus": [
        {
          "type": "webview",
          "id": "deepseek-nexus.chatView",
          "name": "DeepSeek Chat"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "deepseek-nexus",
          "title": "DeepSeek Nexus",
          "icon": "$(comment-discussion)"
        }
      ]
    },
      "configuration": {
        "title": "DeepSeek Nexus",
        "properties": {
          "deepseek.apiKey": {
            "type": "string",
            "default": "",
            "description": "SiliconFlow API Key for authentication",
            "markdownDescription": "Your SiliconFlow API key. Get one at [siliconflow.cn](https://siliconflow.cn) for free access to DeepSeek models"
          },
          "deepseek.apiKeys.siliconflow": {
            "type": "string",
            "default": "",
            "description": "SiliconFlow API Key",
            "markdownDescription": "API key for SiliconFlow"
          },
          "deepseek.apiKeys.openai": {
            "type": "string",
            "default": "",
            "description": "OpenAI API Key",
            "markdownDescription": "API key for OpenAI"
          },
          "deepseek.apiKeys.openrouter": {
            "type": "string",
            "default": "",
            "description": "OpenRouter API Key",
            "markdownDescription": "API key for OpenRouter"
          },
          "deepseek.apiKeys.moonshot": {
            "type": "string",
            "default": "",
            "description": "Moonshot(Kimi) API Key",
            "markdownDescription": "API key for Moonshot(Kimi)"
          },
          "deepseek.provider": {
            "type": "string",
            "default": "siliconflow",
            "description": "Select AI provider",
            "enum": [
              "siliconflow",
              "openai",
              "openrouter",
              "moonshot"
            ],
            "enumDescriptions": [
              "SiliconFlow (DeepSeek 免费接入)",
              "OpenAI 官方",
              "OpenRouter 聚合",
              "Kimi (Moonshot)"
            ]
          },
          "deepseek.model": {
            "type": "string",
            "default": "deepseek-ai/DeepSeek-V3",
            "description": "Select SiliconFlow Model (V3 is Chat, R1 is Reasoning)",
            "enum": [
              "deepseek-ai/DeepSeek-V3",
              "deepseek-ai/DeepSeek-R1",
              "deepseek-ai/DeepSeek-V2.5"
            ],
            "enumDescriptions": [
              "Latest general purpose chat model (recommended)",
              "Advanced reasoning model with thinking process",
              "Previous version with good performance"
            ]
          },
          "deepseek.baseUrl": {
            "type": "string",
            "default": "https://api.siliconflow.cn/v1",
            "description": "SiliconFlow API base URL (free DeepSeek access)"
          }
        }
      }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./dist/test/runTest.js",
    "build-webview": "cd webview-ui && npm run build"
  },
  "devDependencies": {
    "@types/node": "^16.18.126",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "openai": "^6.16.0"
  }
}
